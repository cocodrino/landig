<section class="wrapper style2 container special-alt" id="iconos_container">
    <div class=" row 40% uk'grid">
        <header>
            <h2>{{ page.header.title }}</h2>
        </header>
        <ul class="icons_box uk-grid">
            {% for icons in page.header.icons %}
                {# <li><span class="icon fa-{{ icons.icon }}"><span class="label">{{ icons.label }}</span></span></li>#}
                <li class="
                uk-width-large-1-5
                 uk-width-medium-1-5
                 uk-width-small-1-3">

                    <img class="icons_icon" src="/user/themes/twenty/images/{{ icons.icon }}" alt="">
                    <div class="icon_box">
                        <span class="label icons-label">{{ icons.label }}</span></span>
                    </div>
                </li>
            {% endfor %}
        </ul>
    </div>
</section>
